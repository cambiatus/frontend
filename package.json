{
  "name": "frontend",
  "version": "1.2.0",
  "private": true,
  "engines": {
    "node": "11.15.0"
  },
  "scripts": {
    "build": "yarn transform && npx elm-app build",
    "transform": "postcss src/styles/main.css -o src/styles/processed.css",
    "start": "yarn transform && standard && npx elm-app start",
    "make": "elm make",
    "repl": "elm repl",
    "reactor": "elm reactor",
    "test": "npx elm-test",
    "generate-graphql": "npx elm-graphql https://api.bespiral.io/api/graph --base Bespiral --output src/elm"
  },
  "standard": {
    "plugins": [
      "html"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "docker/env-config.js",
      "scripts/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
